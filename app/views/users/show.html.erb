<h1><%= @user.email %>'s Profile</h1>
<% if @wip.any? %>
  <h2>Works in Progress</h2>
  <% @wip.each do |project| %>
    <%= image_tag project.artwork_iterations.last.image.url %>
  <% end %>
<% end %>
<% if @projects %>
  <h2>Projects</h2>
  <% @projects.each do |project| %>
    <%= image_tag project.artwork_iterations.last.image.url %>
  <% end %>
<% end %>