<h1>Listing projects</h1>
<div class="fluid-container">
  <div class="row">
    <% @projects.each do |project| %>
      <div class="col-md-3">
      <%= link_to(project, 
        title: project.name, 
        rel: 'tooltip', relative: true) do %>
        <%= image_tag(project.artwork_iterations.last.image.url(:medium), 
        alt: "#{project.artwork_iterations.last.name}",
        class: 'img-responsive') %>
      <% end %>
      </div>
    <% end %>
  </div>
</div>
